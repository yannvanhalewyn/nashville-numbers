<h1>WELCOME TO SHEETS PAGE {{{num}}}</h1>
<p>You don't have any sheets yet!</p>
<form action="/sheets" method="POST">
  <label for="title">Title</label>
  <input id="title" type="text" name="title">
  <label for="artist">Artist</lavel>
    <input id="artist" type="text" name="artist">
    <input class="btn" type="submit" value="New Sheet">
</form>

<ul class="sheets-listing clearFloats">
  {{#each sheets}}
  <li class="sheet-list-entry">
    <div class="block-thumb"><span class="fa fa-file-o"></span></div>
    <div class="block-info">
      <div class="title">{{this.title}}</div>
      <div class="artist">{{this.artist}}</div>
      <div class="info">
        <span class="fa fa-globe"></span> public <em>modified 3 hours ago</em>
      </div>
      <div class="actions">
        <span class="fa fa-cloud-download"></span>
        <span class="fa fa-trash-o"></span>
      </div>
    </div>
  </li>
  {{/each}}
</ul>
